<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox fx:id="contentVBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="builder.visframe.fileformat.record.utils.StringMarkerBuilderEmbeddedUIContentController">
   <children>
      <HBox>
         <children>
            <HBox>
               <children>
                  <Label text="type" />
                  <ChoiceBox fx:id="typeChoiceBox" prefWidth="150.0" />
               </children>
            </HBox>
            <CheckBox fx:id="isCaseSensitiveCheckBox" mnemonicParsing="false" text="case sensitive" />
         </children>
      </HBox>
      <HBox>
         <children>
            <Label text="String" />
            <TextField fx:id="stringValueTextField" prefHeight="31.0" prefWidth="254.0" />
         </children>
      </HBox>
   </children>
</VBox>
