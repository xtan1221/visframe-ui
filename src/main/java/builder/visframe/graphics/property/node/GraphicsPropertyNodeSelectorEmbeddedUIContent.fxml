<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox fx:id="rootVBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="builder.visframe.graphics.property.node.GraphicsPropertyNodeSelectorEmbeddedUIContentController">
   <children>
      <HBox>
         <children>
            <Label prefHeight="21.0" prefWidth="100.0" text="major type" />
            <ComboBox fx:id="majorTypeComboBox" prefWidth="300.0" />
         </children>
      </HBox>
      <HBox>
         <children>
            <Label prefHeight="21.0" prefWidth="100.0" text="property type" />
            <TreeView fx:id="propertyNodeTreeView" prefHeight="200.0" showRoot="false" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox>
         <children>
            <Button fx:id="clearSelectionButton" mnemonicParsing="false" onAction="#clearSelectionButtonOnAction" text="clear selection" />
         </children>
      </HBox>
   </children>
</VBox>
