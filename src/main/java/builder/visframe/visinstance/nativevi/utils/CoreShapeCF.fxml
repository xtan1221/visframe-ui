<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<HBox fx:id="rootContainerHBox" alignment="CENTER" spacing="50.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="builder.visframe.visinstance.nativevi.utils.CoreShapeCFController">
   <children>
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <Label text="index" />
            <TextField fx:id="cfIndexTextField" editable="false" prefWidth="100.0" />
            <Button fx:id="viewDetailButton" mnemonicParsing="false" onAction="#viewDetailButtonOnAction" text="view detail" />
            <RadioButton fx:id="assignedMandatoryTargetRadioButton" mnemonicParsing="false" mouseTransparent="true" text="assigned mandatory target" />
         </children>
      </HBox>
      <HBox alignment="CENTER">
         <children>
            <CheckBox fx:id="toIncludeInVisInstanceCheckBox" mnemonicParsing="false" onAction="#toIncludeInVisInstanceCheckBoxOnAction" style="-fx-background-color: red;" text="to include in VisInstance" />
         </children>
      </HBox>
   </children>
</HBox>
