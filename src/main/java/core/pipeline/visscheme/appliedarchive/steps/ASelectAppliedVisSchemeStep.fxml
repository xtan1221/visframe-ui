<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<HBox fx:id="rootContainerHBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="core.pipeline.visscheme.appliedarchive.steps.ASelectAppliedVisSchemeStepController">
   <children>
      <VBox>
         <children>
            <HBox>
               <children>
                  <HBox>
                     <children>
                        <Label text="applied VisScheme UID" />
                        <TextField fx:id="appliedVisSchemeUIDTextField" editable="false" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label text="applied VisScheme name" />
                        <TextField fx:id="appliedVisSchemeNameTextField" editable="false" />
                     </children>
                  </HBox>
                  <Button fx:id="viewDetailButton" mnemonicParsing="false" onAction="#viewDetailButtonOnAction" text="view detail" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT">
               <children>
                  <Button fx:id="chooseAppliedVisSchemeButton" mnemonicParsing="false" onAction="#chooseAppliedVisSchemeButtonOnAction" text="brows.." />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
