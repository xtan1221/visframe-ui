<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<HBox fx:id="rootContainerHBox" style="-fx-border-color: red;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="core.table.hasIDTypeRelationalTableSchema.column.ColumnSorterController">
   <children>
      <Label text="name" />
      <TextField fx:id="columnNameTextField" editable="false" />
      <ToggleButton fx:id="sortByASCToggleButton" mnemonicParsing="false" selected="true" text="By ASC" />
      <Button fx:id="moveUpButton" mnemonicParsing="false" onAction="#moveUpButtonOnAction" text="^" />
      <Button fx:id="moveDownButton" mnemonicParsing="false" onAction="#moveDownButtonOnAction" text="v" />
      <CheckBox fx:id="includedCheckBox" mnemonicParsing="false" text="selected" />
   </children>
   <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
   </padding>
</HBox>
