<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<StackPane fx:id="dagNodeRootStackPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dependencygraph.cfd2.SimpleCFDGraphNodeController">
   <children>
      <Circle fill="DODGERBLUE" radius="50.0" stroke="BLACK" strokeType="INSIDE" />
      <VBox alignment="CENTER">
         <children>
            <Label fx:id="dagNodeInforLabel" text="Label" />
            <Button fx:id="viewNodeDetailsButton" mnemonicParsing="false" onAction="#viewNodeDetailsButtonOnAction" text="view" />
            <HBox fx:id="copyNodesHBox" alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding></HBox>
         </children>
      </VBox>
   </children>
</StackPane>
