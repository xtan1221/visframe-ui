<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<StackPane fx:id="dagNodeRootStackPane" style="-fx-border-color: red; -fx-border-width: 3;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dependencygraph.solutionset.DAGNodeController">
   <children>
      <Circle fx:id="backgroundCircle" fill="DODGERBLUE" radius="40.0" stroke="BLACK" strokeType="INSIDE" />
      <VBox alignment="CENTER" StackPane.alignment="CENTER">
         <children>
            <Label fx:id="dagNodeInforLabel" text="Label">
               <font>
                  <Font size="10.0" />
               </font></Label>
            <Button fx:id="viewNodeDetailsButton" mnemonicParsing="false" onAction="#viewNodeDetailsButtonOnAction" text="view details">
               <font>
                  <Font size="10.0" />
               </font></Button>
            <Button fx:id="selectionButton" mnemonicParsing="false" onAction="#selectionButtonOnAction" text="select">
               <font>
                  <Font size="10.0" />
               </font></Button>
         </children>
      </VBox>
   </children>
</StackPane>
