<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox fx:id="rootContainerVBox" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewer.visframe.function.variable.independent.IndependentFreeInputVariableTypeViewerController">
   <children>
      <HBox>
         <children>
            <Label text="name" />
            <TextField fx:id="nameTextField" editable="false" prefWidth="200.0" />
         </children>
      </HBox>
      <HBox>
         <children>
            <Label text="notes" />
            <TextArea fx:id="notesTextArea" editable="false" prefHeight="100.0" prefWidth="600.0" />
         </children>
      </HBox>
      <HBox>
         <children>
            <Label text="Data type" />
            <TextField fx:id="dataTypeTextField" editable="false" prefWidth="200.0" />
         </children>
      </HBox>
      <VBox>
         <children>
            <Label text="Owner CompositionFunction" />
            <VBox fx:id="ownerCFVBox" prefHeight="200.0" prefWidth="100.0" />
         </children>
      </VBox>
   </children>
</VBox>
