<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox fx:id="rootContainerVBox" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewer.visframe.graphics.shape.VfShapeTypeViewerController">
   <children>
      <HBox>
         <children>
            <HBox>
               <children>
                  <Label text="name" />
                  <TextField fx:id="shapeTypeNameTextField" editable="false" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label text="notes" />
                  <TextArea fx:id="notesTextArea" editable="false" prefHeight="50.0" prefWidth="200.0" VBox.vgrow="NEVER" />
               </children>
            </HBox>
         </children>
      </HBox>
      <ScrollPane fitToHeight="true" fitToWidth="true">
         <content>
            <VBox fx:id="graphicsPropertyTreeVBox" />
         </content>
      </ScrollPane>
   </children>
</VBox>
