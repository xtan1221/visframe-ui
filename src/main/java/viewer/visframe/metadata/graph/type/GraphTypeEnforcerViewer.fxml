<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<GridPane fx:id="rootContainerGridPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewer.visframe.metadata.graph.type.GraphTypeEnforcerViewerController">
   <children>
      <CheckBox fx:id="toForceDirectedCheckBox" mnemonicParsing="false" mouseTransparent="true" text="toForceDirected" />
      <CheckBox fx:id="toForceUndirectedCheckBox" mnemonicParsing="false" mouseTransparent="true" text="toForceUndirected" GridPane.rowIndex="1" />
      <CheckBox fx:id="toForceNoParallelEdgesCheckBox" mnemonicParsing="false" mouseTransparent="true" text="toForceNoParallelEdges" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <CheckBox fx:id="toForceNoSelfLoopsCheckBox" allowIndeterminate="true" mnemonicParsing="false" mouseTransparent="true" text="toForceNoSelfLoops" GridPane.rowIndex="2" />
      <HBox GridPane.columnIndex="1">
         <children>
            <Label text="directedForcingMode" GridPane.columnIndex="1" />
            <TextField fx:id="directedForcingModeTextField" editable="false" />
         </children>
      </HBox>
   </children>
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" />
      <ColumnConstraints hgrow="SOMETIMES" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
</GridPane>
