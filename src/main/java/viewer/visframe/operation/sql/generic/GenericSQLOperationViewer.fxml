<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox fx:id="rootContainerVBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewer.visframe.operation.sql.generic.GenericSQLOperationViewerController">
   <children>
      <GridPane>
         <children>
            <Label text="Operation type" />
            <TextField fx:id="operationTypeTextField" editable="false" GridPane.columnIndex="1" />
            <Label text="Operation instance name" GridPane.rowIndex="1" />
            <TextField fx:id="operationInstanceNameTextField" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" />
            <ColumnConstraints hgrow="SOMETIMES" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
      <Label text="notes" />
      <TextArea fx:id="notesTextArea" editable="false" prefHeight="100.0" />
      <HBox>
         <children>
            <Label text="outputRecordDataName" />
            <TextField fx:id="outputRecordDataNameTextField" editable="false" prefWidth="200.0" />
         </children>
      </HBox>
      <Label text="genericSQLQuery" />
      <HBox fx:id="genericSQLQueryViewerHBox" />
   </children>
</VBox>
