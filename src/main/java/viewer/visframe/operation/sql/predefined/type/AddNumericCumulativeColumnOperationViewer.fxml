        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" />
          <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="column name" />
            <Label text="sort order type" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Label text="otherKeptNonPKColumnNameSet" />
      <VBox fx:id="otherKeptNonPKColumnNameSetVBox" />
      <Label text="cumulativeColumnSymjaExpression" />
      <HBox fx:id="cumulativeColumnSymjaExpressionViewerHBox" />
      <HBox>
         <children>
            <Label text="cumulativeColumnNameInOutputDataTable" />
            <TextField fx:id="cumulativeColumnNameInOutputDataTableTextField" editable="false" />
         </children>
      </HBox>
      <HBox>
         <children>
            <Label text="initialValue" />
            <TextField fx:id="initialValueTextField" editable="false" />
         </children>
      </HBox>
   </children>
